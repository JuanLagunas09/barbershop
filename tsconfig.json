{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/components": ["src/components/*"],
      "@/layouts": ["src/layouts/*"],
      "@/pages": ["src/pages/*"],
      "@/styles": ["src/styles/*"],
      "@/scripts": ["src/scripts/*"],
      "@/icons": ["src/icons/*"],
    }
  }
}